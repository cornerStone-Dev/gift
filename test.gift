(def factorial
	(.\ (n)
		(if (= n 1)
			1
			(* n (factorial (- n 1)))
		)
	)
)
(factorial 4)

(def fact-iter (.\ (product n)
	(if (< n 2)
		(+ product 0)
		(fact-iter (* product n) (- n 1)))) )
(fact-iter 1 4)
